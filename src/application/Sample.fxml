<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane id="BorderPane" stylesheets="@application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane fx:id="basePane" prefHeight="500.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addButton" layoutX="701.0" layoutY="462.0" mnemonicParsing="false" onAction="#addAccountButtonPushed" prefHeight="25.0" prefWidth="88.0" text="Add Account" />
         </children></AnchorPane>
   </center>
   <left>
      <VBox fx:id="vboxleft" prefHeight="501.0" prefWidth="121.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="68.0" prefWidth="121.0" />
            <Button mnemonicParsing="false" onAction="#buttonPushed" prefHeight="25.0" prefWidth="139.0" text="Button" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="Button" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="Button" />
            <Button mnemonicParsing="false" onAction="#changeScreenButtonPushed" prefHeight="25.0" prefWidth="139.0" text="Button" />
         </children>
      </VBox>
   </left>
   <top>
      <HBox fx:id="hboxtop" BorderPane.alignment="CENTER">
         <children>
            <MenuBar prefHeight="25.0" prefWidth="132.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Region prefHeight="25.0" prefWidth="759.0" visible="false" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#handleCloseButtonAction">
               <graphic>
                  <ImageView fitHeight="14.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../Downloads/icons8-delete-32.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
